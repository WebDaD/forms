          li.nav-item.active
            a.nav-link(href="/admin/") Dashboard
          li.nav-item
            a.nav-link(href="/admin/#!/personal_registration") Personal Registration
      #ng-view(ng-view="")
    script(src="/js/angular.min.js")
    script(src="/js/angular-route.min.js")
    script(src="/js/angular-cookies.min.js")
    script(src="/js/angular-animate.min.js")
    script(src="/js/ui-bootstrap-tpls.js")
    script(src="/js/admin/app.js")
    script(src="/js/admin/svc.js")
    script(src="/js/admin/controllers/dashboard.js")
    script(src="/js/admin/controllers/personal_registration.js")
